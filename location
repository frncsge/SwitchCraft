    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Location Page</title>
</head>
<body>
    <nav id="navbar"></nav>
    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3933.3009212593183!2d123.86706127292776!3d9.655308478875247!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x33aa4c30923688e3%3A0xbf744e652643049b!2sIsland%20City%20Mall!5e0!3m2!1sen!2sph!4v1745756867663!5m2!1sen!2sph" width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
    <footer id="footer"></footer>

    <script>
        fetch("/Nav and footer components/navigationBar.html")
          .then((x) => x.text())
          .then((y) => {
            document.getElementById("navbar").innerHTML = y;
            const script = document.createElement("script");
            const jquery = document.createElement("script");
  
            jquery.src =
              "https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js";
            script.src = "/Nav and footer components/navbar.js";
  
            document.body.appendChild(jquery);
            document.body.appendChild(script);
          });
  
        fetch("/Nav and footer components/footer.html")
          .then((x) => x.text())
          .then((y) => (document.getElementById("footer").innerHTML = y));
      </script>
</body>
</html>
